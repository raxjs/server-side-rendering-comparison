    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.2.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "benchmark": "^2.1.3",
    "marko-loader": "^1.2.0",
    "nodemon": "^1.11.0",
    "webpack": "^1.13.3"
  }
