  },
  "author": {
    "name": "imsobear"
  },
  "dependencies": {
    "inferno": "latest",
    "inferno-component": "latest",
    "inferno-create-element": "latest",
    "inferno-server": "latest",
    "koa": "^1.2.4",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "marko": "latest",
    "preact": "latest",
    "preact-render-to-string": "latest",
    "rapscallion": "^2.1.5",
    "rax": "^0.4.16",
    "rax-server-renderer": "^0.4.16",
    "react": "latest",
    "react-dom": "latest",
    "vue": "latest",
